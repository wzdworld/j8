<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.star.mkdocshelper.controller.material.center.FaviconController">
   <children>
      <Button fx:id="openFileSelectorBtn" layoutX="63.0" layoutY="102.0" mnemonicParsing="false" onAction="#openFileSelector" text="上传" />
      <Label layoutX="63.0" layoutY="38.0" text="设置Favicon图片" />
      <ImageView fx:id="faviconImageViewSelected" fitHeight="150.0" fitWidth="200.0" layoutX="58.0" layoutY="155.0" pickOnBounds="true" preserveRatio="true" />
      <Button layoutX="62.0" layoutY="327.0" mnemonicParsing="false" onAction="#saveFaviconSettings" text="保存" />
   </children>
</AnchorPane>

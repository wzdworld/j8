<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.star.mkdocshelper.controller.material.center.LogoController">
   <children>
      <Button fx:id="openFileSelectorBtn" layoutX="73.0" layoutY="112.0" mnemonicParsing="false" onAction="#openFileSelector" text="上传" />
      <Label layoutX="73.0" layoutY="48.0" text="设置Logo" />
      <ImageView fx:id="logoImageViewSelected" fitHeight="150.0" fitWidth="200.0" layoutX="68.0" layoutY="165.0" pickOnBounds="true" preserveRatio="true" />
      <Button layoutX="72.0" layoutY="337.0" mnemonicParsing="false" onAction="#saveLogo" text="保存" />
   </children>
</AnchorPane>
